template bgp tpl_bgp_iana_base {
  local as OWNAS;
  path metric 1;
  direct;
  ipv4 { table bgpianatable4; import none; export none; };
  ipv6 { table bgpianatable6; import none; export none; };
}

template bgp tpl_bgp_iana_downstream_4 from tpl_bgp_iana_base {
  ipv4 {
    import where iana_downstream_import_filter();
    export where iana_downstream_export_filter();
  };
}

